quire_symbols":[],"replace_symbols":[],"code":"\/* AT7: Testiramo funkciju izbaci_skupe *\/\n\/* NOTE: funkcija vraca broj elemenata nakon izbacivanja! *\/\n\nstruct Prodavnica p[] = {\n\t{\"Konzum\", 2}, \/\/Konzum ima vece cijene!\n\t{\"Amko\", 1}, \/\/jedino on ostaje\n\t{\"Bingo\", 3}, \/\/i Bingo ima vece cijene zbog Graha (5KM)\n};\nstruct Artikal n[] = {\n\t{\"Brasno\", 5.2, 1},\n\t{\"Ulje\", 2, 1},\n\t{\"Secer\", 1.5, 1},\n\t{\"Brasno\", 5.2, 2},\n\t{\"Ulje\", 2.1, 2},\n\t{\"Secer\", 2.2, 2},\n\t{\"Brasno\", 5.2, 3},\n\t{\"Ulje\", 1.1, 3},\n\t{\"Secer\", 1.2, 3},\n\t{\"Grah\", 5, 3}\n};\nint nova_vel = izbaci_skupe(p, 3, n, 10), i;\nprintf (\"Ostalo je %d prodavnica, i to: \\n\", nova_vel);\nfor (i = 0; i < nova_vel; ++i)\n\tprintf (\"'%s', ID: %d\\n\", p[i].naziv, p[i].ID);","global_top":"","global_above_main":"","running_params":{"timeout":10,"vmem":1000000,"stdin":""},"expected":["Ostalo je 1 prodavnica, i to: \\n'Amko', ID: 1"],"expected_exception":"false","expected_crash":"false","ignore_whitespace":"false","regex":"false","substring":"false"},{"id":"6128","require_symbols":[],"replace_symbols":[],"code":"\/* AT8: ID-ovi prodavnica ne odgovaraju artiklima (prodavnice NEMAJU artikala), nema se sta izbacivati u tom slucaju! *\/\t\r\nstruct Prodavnica p[] = {\r\n\t{\"Amko\", 1},\r\n\t{\"Bingo\", 2},\r\n\t{\"Konzum\", 3},\r\n};\r\nstruct Artikal a[] = {\r\n\t{\"Kiseli kupus\", 10, 4},\r\n\t{\"Bickl\", 1000, 5},\r\n\t{\"Krastavci\", 2, 4}\r\n};\r\nint i = izbaci_skupe(p, 3, a, 3), j;\r\nfor (j = 0; j < i; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\r\n\r\n\/* Kontrola za funkciju koja ne radi nista *\/\r\na[0].prodavnica=1;\r\na[1].prodavnica=2;\r\na[3].prodavnica=1;\r\n\r\ni = izbaci_skupe(p, 3, a, 3);\r\nfor (j = 0; j < i; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\t","global_top":"","global_above_main":"","running_params":{"timeout":10,"vmem":1000000,"stdin":""},"expected":["Amko, ID: 1\\nBingo, ID: 2\\nKonzum, ID: 3\\nAmko, ID: 1\\nKonzum, ID: 3"],"expected_exception":"false","expected_crash":"false","ignore_whitespace":"false","regex":"false","substring":"false"},{"id":"6129","require_symbols":[],"replace_symbols":[],"code":"\/* AT9: Broj artikala ili broj prodavnica je 0  *\/\t\r\nstruct Prodavnica p[] = {\r\n\t{\"Amko\", 1},\r\n\t{\"Bingo\", 2},\r\n\t{\"Konzum\", 3},\r\n};\r\nstruct Artikal a[] = {\r\n\t{\"Kiseli kupus\", 10, 4},\r\n\t{\"Bickl\", 1000, 5},\r\n\t{\"Krastavci\", 2, 4}\r\n};\r\nint i = izbaci_skupe(p, 3, a, 0), j;\r\nfor (j = 0; j < i; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\r\ni = izbaci_skupe(p, 0, a, 0);\r\nfor (j = 0; j < 3; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\r\ni = izbaci_skupe(p, 0, a, 3);\r\nfor (j = 0; j < 3; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\t\r\n\r\n\/* Kontrola za funkciju koja ne radi nista *\/\r\na[0].prodavnica=1;\r\na[1].prodavnica=2;\r\na[3].prodavnica=1;\r\n\r\ni = izbaci_skupe(p, 3, a, 3);\r\nfor (j = 0; j < i; ++j)\r\n\tprintf (\"%s, ID: %d\\n\", p[j].naziv, p[j].ID);\t\r\n","global_top":"","global_above_main":"","running_params":{"timeout":10,"vmem":1000000,"stdin":""},"expected":["Amko, ID: 1\\nBingo, ID: 2\\nKonzum, ID: 3\\nAmko, ID: 1\\nBingo, ID: 2\\nKonzum, ID: 3\\nAmko, ID: 1\\nBingo, ID: 2\\nKonzum, ID: 3\\nAmko, ID: 1\\nKonzum, ID: 3"],"expected_exception":"false","expected_crash":"false","ignore_whitespace":"false","regex":"false","substring":"false"},{"id":"6130","require_symbols":[],"replace_symbols":[],"code":"\/* AT10 (skriveni): SVE ZAJEDNO *\/\t\r\nstruct Artikal a[20], sir = {\"Sir\", 0.1, 3};\r\nstruct Prodavnica p[20];\r\nint vel_a, vel_p, uspjesno;\r\nvel_a = ucitaj_artikle(a, 20);\r\n\r\na[vel_a - 1] = sir; \/* sada Bingo ima jeftini sir umjesto graha *\/\r\na[0].cijena = 2; \/* cijena Brasna u Amku je 2KM *\/\r\nFILE *f = fopen (\"prodavnice.txt\", \"w\");\r\nfprintf (f, \"Amko,1\\nKonzum,2\\nBingo,3\");\r\nfclose (f);\r\nvel_p = ucitaj_prodavnice(p, 20);\r\nvel_p = izbaci_skupe(p, vel_p, a, vel_a);\r\nuspjesno = sacuvaj(p, vel_p, \"dat.bin\");\r\nprintf (\"Uspjesno: %d, vel_a: %d, vel_p: %d\\n\", uspjesno, vel_a, vel_p);\r\nf = fopen (\"dat.bin\", \"rb\");\r\nfseek (f, 0, SEEK_END);\r\n\/* ostali su amko (1.83 prosjek) i bingo (1.9), konzum izbacen (3.16), dok\r\n   je prosjek artikala 2.26\r\n*\/\r\nprintf (\"Uspjesno snimljeno bajta: %d\", (int)ftell(f));\r\nfclose (f);","global_top":"","global_above_main":"","running_params":{"timeout":10,"vmem":1000000,"stdin":""},"expected":["Uspjesno: 1, vel_a: 10, vel_p: 2\\nUspjesno snimljeno bajta: 208"],"expected_exception":"false","expected_crash":"false","ignore_whitespace":"false","regex":"false","substring":"false"}]}