<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.SendFile">

    <stylesheets>
        <URL value="@some.css" />
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints maxWidth="200" minWidth="200" prefWidth="200" />
        <ColumnConstraints maxWidth="200" minWidth="200" prefWidth="200" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30" minHeight="30" prefHeight="30" />
        <RowConstraints maxHeight="30" minHeight="30" prefHeight="30" />
        <RowConstraints maxHeight="30" minHeight="30" prefHeight="30" />
        <RowConstraints maxHeight="30" minHeight="30" prefHeight="30" />
        <RowConstraints maxHeight="30" minHeight="30" prefHeight="30" />
    </rowConstraints>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
    </padding>

    <Label alignment="CENTER" maxHeight="30" maxWidth="200" minHeight="30" minWidth="200" prefHeight="30" prefWidth="200" text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label alignment="CENTER" maxHeight="30" maxWidth="200" minHeight="30" minWidth="200" prefHeight="30" prefWidth="200" text="Surname:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Label alignment="CENTER" maxHeight="30" maxWidth="200" minHeight="30" minWidth="200" prefHeight="30" prefWidth="200" text="Adress:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <Label alignment="CENTER" maxHeight="30" maxWidth="200" minHeight="30" minWidth="200" prefHeight="30" prefWidth="200" text="Town:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Label alignment="CENTER" maxHeight="30" maxWidth="200" minHeight="30" minWidth="200" prefHeight="30" prefWidth="200" text="Post number:" GridPane.columnIndex="0" GridPane.rowIndex="4" />
    <TextField fx:id="nameTextField" styleClass="size" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <TextField fx:id="surnameTextField" styleClass="size" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="adressTextField" styleClass="size" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <TextField fx:id="townTextField" styleClass="size" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <TextField fx:id="postNumberTextField" styleClass="size" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   <opaqueInsets>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </opaqueInsets>
</GridPane>
