<TabPane maxHeight="360" maxWidth="600" minHeight="360" minWidth="600" prefHeight="360" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.Geografija">
  <tabs>
    <Tab text="Pretraga">
         <content>
            <GridPane>
              <columnConstraints>
                  <ColumnConstraints halignment="CENTER" maxWidth="150" minWidth="150.0" prefWidth="150.0" />
