                <Label styleClass="label" text="Grad" GridPane.columnIndex="0" GridPane.columnSpan="3" GridPane.rowIndex="2" />
                <Label styleClass="label" text="Naziv:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
