              </rowConstraints>
               <padding>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </padding>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>

                <Label styleClass="label" text="Naziv:" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.rowSpan="2" />
                <TextField fx:id="nazivTextField" promptText="unesite naziv drzave" styleClass="textfield" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0" GridPane.rowSpan="2" />
                <Button fx:id="nadjiDrzavuButton" maxHeight="30" maxWidth="100" minHeight="30" minWidth="100" onAction="#nadjiDrzavuClicked" prefHeight="30" prefWidth="100" text="NADJI DRZAVU" GridPane.columnIndex="3" GridPane.rowIndex="1" />

                <Label styleClass="label" text="Drzava" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="2" />
                <Label styleClass="label" text="Naziv:" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                <TextField fx:id="nazivDrzavePretraga" editable="false" promptText="rezultat pretrage" styleClass="textfield" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />

                <Label styleClass="label" text="Grad" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="4" />
                <Label text="Naziv:" GridPane.columnIndex="0" GridPane.rowIndex="5" />
                <TextField fx:id="nazivGradaPretraga" promptText="rezultat pretrage" styleClass="textfield" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5" />
                <Label text="Broj stanovnika:" GridPane.columnIndex="0" GridPane.rowIndex="6" />
                <TextField fx:id="brojStanovnikaPretraga" promptText="rezultat pretrage" styleClass="textfield" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="6" />
                <Label fx:id="porukaPretraga" alignment="CENTER" maxHeight="30" maxWidth="600" minHeight="30" minWidth="600" prefHeight="30" prefWidth="600" textFill="#ee0303" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="7" />

